{{#if (eq type 'enum')}}
  {{common/enum-drop-down fieldName=fieldName name=name data-style=data-style changeset=changeset parent=parent
                          data-live-search=data-live-search emptyOption=emptyOption value=fieldValue change-function=change-function}}
{{else if (eq type 'select')}}
  {{common/select-dropdown-field fieldName=fieldName tableName=tableName columnKey=columnKey columnValue=columnValue changeset=changeset change-function=change-function
                                 parent=parent value=fieldValue data-style=data-style data-live-search=data-live-search emptyOption=emptyOption}}
{{else}}
  {{common/drop-down fieldName=fieldName content=content data-style=data-style data-live-search=data-live-search changeset=changeset
                     value=fieldValue parent=parent change-function=change-function}}
{{/if}}

{{common/message-alert changeset=changeset fieldName=fieldName}}